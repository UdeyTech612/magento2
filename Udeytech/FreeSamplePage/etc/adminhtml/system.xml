                </field>
                <field id="7free_sample_bundle_product" showInDefault="1" showInStore="1" showInWebsite="1"
                       sortOrder="10" translate="label" type="select">
                    <label>7 Free sample bundle product</label>
                    <comment/>
                    <source_model>Udeytech\FreeSamplePage\Model\Config\Source\Fspbundle</source_model>
                </field>
            </group>
        </section>
    </system>
</config>

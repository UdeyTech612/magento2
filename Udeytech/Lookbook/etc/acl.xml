<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Udeytech_Lookbook::Positions" sortOrder="10" title="Positions">
                    <resource id="Udeytech_Lookbook::Positions_save" sortOrder="10" title="Save Positions"/>
                    <resource id="Udeytech_Lookbook::Positions_delete" sortOrder="20" title="Delete Positions"/>
                    <resource id="Udeytech_Lookbook::Positions_update" sortOrder="30" title="Update Positions"/>
                    <resource id="Udeytech_Lookbook::Positions_view" sortOrder="40" title="View Positions"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Udeytech_Lookbook::config_udeytech_lookbook" title="lookbook"/>
                        </resource>
                    </resource>
                </resource>
                <resource id="Udeytech_Lookbook::udeytech_lookbook" sortOrder="10" title="udeytech lookbook">
                    <resource id="Udeytech_Lookbook::index_index" sortOrder="10" title="index index"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
